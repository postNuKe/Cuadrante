<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/line_a"
            style="@style/servicio_list_item_line_a" />

        <TextView
            android:id="@+id/line_b"
            style="@style/servicio_list_item_line_b" />

        <TextView
            android:id="@+id/line_c"
            style="@style/servicio_list_item_line_b"
            android:visibility="gone" />

        <TextView
            android:id="@+id/line_d"
            style="@style/servicio_list_item_line_b"
            android:visibility="gone" />

        <TextView
            android:id="@+id/line_e"
            style="@style/servicio_list_item_line_b"
            android:visibility="gone" />

        <TextView
            android:id="@+id/line_f"
            style="@style/servicio_list_item_line_b"
            android:visibility="gone" />

        <TextView
            android:id="@+id/line_g"
            style="@style/servicio_list_item_line_b"
            android:visibility="gone" />

        <TextView
            android:id="@+id/line_h"
            style="@style/servicio_list_item_line_b"
            android:visibility="gone" />

        <Spinner
            android:id="@+id/spinner"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone" />
    </LinearLayout>

</RelativeLayout>